<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="163dp"
    android:layout_marginStart="8.5dp"
    android:layout_marginEnd="8.5dp"
    android:layout_marginBottom="@dimen/dp_16"
    android:background="@drawable/white_tuo">

    <ImageView
        android:id="@+id/image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_16"
        android:src="@drawable/img_home_key" />

    <TextView
        android:id="@+id/test_keyName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/image"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginEnd="@dimen/dp_20"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/set_txt"
        android:textSize="@dimen/sp_15"
        tools:text="BixinKey123" />

    <TextView
        android:id="@+id/test_blueName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/test_keyName"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginEnd="@dimen/dp_20"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/set_txt"
        android:textSize="@dimen/sp_13"
        tools:text="BixinKey123" />

</RelativeLayout>
